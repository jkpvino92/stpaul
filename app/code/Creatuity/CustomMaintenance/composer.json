{
    "name": "creatuity/magento2-custom-maintenance",
    "description": "Creatuity Custom Maintenance for Magento 2",
    "type": "magento2-component",
    "authors": [
        {
            "name": "Creatuity Corp",
            "homepage": "http://creatuity.com/"
        }
    ],
    "require": {
        "php": "~5.5.0|>=5.6.0",
        "magento/magento-composer-installer": "*"        
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Creatuity\\CustomMaintenance\\": ""
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "map": [
            [
                "pub/errors/creatuity_maintenance",
                "pub/errors/creatuity_maintenance"
            ]
        ]
    }
}
